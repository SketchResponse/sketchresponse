{
  "name": "sketch2",
  "version": "0.0.1",
  "private": true,
  "description": "A graphical input tool for use on the edX platform",
  "author": "Martin Segado",
  "license": "All rights reserved",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "glob": "^5.0.14",
    "gobble": "^0.10.2",
    "gobble-autoprefixer": "^0.1.1",
    "gobble-babel": "^5.5.8",
    "gobble-cli": "^0.4.2",
    "gobble-concat": "^0.3.0",
    "gobble-rename": "^0.1.0",
    "gobble-replace": "^0.3.1",
    "jasmine-core": "^2.3.4",
    "jspm": "^0.16.0-beta.3",
    "karma": "^0.12.37",
    "karma-cli": "0.1.0",
    "karma-jasmine": "^0.3.6",
    "karma-jspm": "git://github.com/Workiva/karma-jspm.git#master",
    "karma-phantomjs-launcher": "~0.2.0",
    "karma-phantomjs-shim": "~1.0.0",
    "node-sass": "^3.5.1",
    "phantomjs": "^1.9.17",
    "postcss-cli": "^2.5.1"
  },
  "scripts": {
    "postinstall": "jspm install",
    "test": "karma start --single-run",
    "start": "gobble",
    "build": "gobble build dist -f",
    "css": "rm -rf ./tmp && npm run sass && npm run autoprefixer",
    "sass": "node-sass --output-style compressed ./styles/ --output ./tmp",
    "autoprefixer": "postcss --use autoprefixer --map file --replace ./tmp/*",
    "cleancss": "mv ./tmp/main.css ./dist/main.css && mv ./tmp/main.css.map ./dist/main.css.map && rm -rf ./tmp"
  },
  "jspm": {
    "dependencies": {
      "classnames": "npm:classnames@^2.1.1",
      "d3": "github:mbostock/d3@^3.5.5",
      "dom-shims": "npm:dom-shims@^0.1.4",
      "events": "github:jspm/nodelibs-events@^0.1.1",
      "fastclick": "npm:fastclick@^1.0.6",
      "jquery": "github:components/jquery@^2.1.3",
      "jsondiffpatch": "npm:jsondiffpatch@^0.1.33",
      "mousetrap": "npm:mousetrap@^1.5.3",
      "pepjs": "github:jquery/pep@^0.3.0",
      "simulant": "npm:simulant@^0.1.5"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.3",
      "babel-runtime": "npm:babel-runtime@^5.8.3",
      "core-js": "npm:core-js@^0.9.18"
    }
  }
}
